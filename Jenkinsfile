pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                echo 'Building the project in feature branch...'
            }
        }
        stage('Test') {
            steps {
                echo 'Running tests in feature branch...'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying application from feature branch...'
            }
        }
    }
}
